  "bugs": {
    "url": "https://github.com/jhart25/bogify_app/issues"
  },
  "homepage": "https://github.com/jhart25/bogify_app#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "express": "^4.16.4",
    "if-env": "^1.0.4",
    "mongoose": "^5.4.17",
    "react-spotify-api": "^1.5.1",
    "reactstrap": "^7.1.0",
    "spotify-web-api-js": "^1.2.0"
  }
}
